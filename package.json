{
  "name": "interlens",
  "private": true,
  "packageManager": "pnpm@9.0.0",
  "workspaces": [
    "apps/*",
    "packages/*"
  ],
  "scripts": {
    "install:all": "pnpm -r install",
    "dev:web": "pnpm --dir apps/web start",
    "dev:mcp": "pnpm --dir packages/mcp start:local",
    "build:web": "pnpm --dir apps/web build"
  }
}
