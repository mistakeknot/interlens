    "openapi",
    "systems-thinking",
    "knowledge-graph",
    "creative-problem-solving",
    "dialectic-thinking",
    "cognitive-augmentation",
    "ai-tools"
  ],
  "author": "Mistake Knot",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/mistakeknot/Interlens.git"
  },
  "files": [
    "index.js",
    "cli.js",
    "api-client.js",
    "lib",
    "docs",
    "schemas",
    "examples"
  ],
  "dependencies": {
    "@modelcontextprotocol/sdk": "^0.5.0",
    "node-fetch": "^3.3.2",
    "express": "^4.18.2",
    "cors": "^2.8.5"
